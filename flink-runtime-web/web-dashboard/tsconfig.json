{
  "compileOnSave": false,
  "files": [
    "node_modules/monaco-editor/monaco.d.ts",
    "node_modules/@antv/g2/src/index.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "paths": {
      "services": [
        "src/app/services"
      ],
      "config": [
        "src/app/app.config.ts"
      ],
      "interfaces": [
        "src/app/interfaces"
      ],
      "utils": [
        "src/app/utils"
      ],
      "share/*": [
        "src/app/share/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  }
}
